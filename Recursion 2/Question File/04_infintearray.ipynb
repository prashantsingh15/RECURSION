    "def _findSum(arr, N):\n",
    "    if N <= 0:\n",
    "        return 0\n",
    "    else:\n",
    "        return _findSum(arr, N - 1) + arr[N - 1]\n",
    " \n",
    "\n",
    "arr =[]\n",
    "\n",
    "arr = [1, 2, 3, 4, 5]\n",
    "  \n",
    "\n",
    "N = len(arr)\n",
    "  \n",
    "ans =_findSum(arr,N)\n",
    "print (ans)"
